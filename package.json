{
  "name": "astro-eval",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "cross-env BASE_PATH=/astro-blog/ astro build",
    "preview": "cross-env BASE_PATH=/astro-blog/ astro preview",
    "gh:preview": "pnpm run build && pnpm run preview",
    "gh:deploy": "pnpm run build && git diff --quiet docs || (git add docs && git commit -m \"build: deploy\" && git push)",
    "astro": "astro"
  },
  "dependencies": {
    "astro": "^5.12.0"
  },
  "devDependencies": {
    "astro-rehype-relative-markdown-links": "^0.18.1",
    "cross-env": "^7.0.3",
    "rimraf": "^6.0.1"
  }
}